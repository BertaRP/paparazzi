<!DOCTYPE module SYSTEM "module.dtd">

<module name="div_ctrl">
  <doc>
    <description></description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings NAME="Divergence_Controller">
        <dl_setting var="gain" min="0" step="0.01" max="1" module="div_ctrl/div_ctrl" shortname="gain"/>
        <dl_setting var="foe_cmd" min="-1" step="0.1" max="1" module="div_ctrl/div_ctrl" shortname="foe_cmd"/>
      </dl_settings>
    </dl_settings>
  </settings>
  
  <header>
    <file name="div_ctrl.h"/>
  </header>
  <init fun="div_ctrl_init()"/>
  <periodic fun="div_ctrl_run()" freq="10" autorun="TRUE"/>
  <makefile>
    <file name="div_ctrl.c"/>
  </makefile>
</module>

